<websiteInformation>	
	<domain>dubizzle.com</domain>
	<start_urls>
		<url>https://uae.dubizzle.com/property-for-sale/residential/?page=4&amp;price__gte=&amp;amp=&amp;price__lte=&amp;amp=&amp;bedrooms__gte=&amp;amp=&amp;bedrooms__lte=&amp;amp=&amp;keywords=&amp;amp=&amp;is_basic_search_widget=0&amp;amp=&amp;is_search=1</url>
	</start_urls>
	<FirstDate>25th Apr 2018</FirstDate>
	<LastDate>03th Jun 2016</LastDate>
	<Category>Apartment</Category>
	<Action>sale</Action>
	<SpiderName>DubbizleApartmentsSale</SpiderName>
	<Source>dubizzle</Source>
	<LogFile>LogFiles/Apartment/Sale/apartments_sale_dubizzle_log.log</LogFile>
	<TemplateAttributes>Websites/Templates/ApartmentsSale.xml</TemplateAttributes>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>MasterPage</Place>
		<DateFunction>DayFirst</DateFunction>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>ad_link</name>
				<xpath>normalize-space(.//span[@class='title']/a/@href)</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>posting_date</name>
				<xpath>normalize-space(.//p[@class='date']/text())</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//div[@id='results-list']/div[@class='list-item-wrapper']</xpath>
		</Containers>
		<NextPage>
			<xpath>string(.//div[@class='paging_forward']/a[@id='next_page']/@href)</xpath>
			<prefix>http://uae.dubizzle.com/</prefix>
		</NextPage>
		<LinkAttribute>
			<name>ad_link</name>
            <action>CorrectLink</action>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>ad_title</name>
			<xpath>normalize-space(.//span[@id='listing-title-wrap']/text())</xpath>
			<type>Selector</type>
		</Attribute>
		<Attribute>
			<name>ad_link</name>
			<defaultValue>NA</defaultValue>			
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<name>posting_date</name>
			<defaultValue>NA</defaultValue>			
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<name>size</name>			
			<type>Selector</type>
			<defaultValue>-1</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields is-flex']//li/span[normalize-space(.)='Size']/following-sibling::strong[1]/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>country</name>
			<type>Selector</type>
			<defaultValue>uae</defaultValue>
			<xpath>there is no value for it</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<name>city</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//span[@id='browse_in_breadcrumb']/div[@class='seo_itemscope']/a/text())</xpath>
            <actions>
                <action>cleanCity</action>
            </actions>
		</Attribute>
		<Attribute>
			<name>bedrooms</name>
			<defaultValue>-1</defaultValue>			
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='important-fields is-flex']//li/span[normalize-space(.)='Bedrooms']/following-sibling::strong[1]/text())</xpath>
			<actions>
                <action>checkIfStudioBedroom</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>bathrooms</name>
			<defaultValue>-1</defaultValue>		
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='important-fields is-flex']//li/span[normalize-space(.)='Bathrooms']/following-sibling::strong[1]/text())</xpath>
			<actions>
                <action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>price_sqft</name>
			<defaultValue>-1</defaultValue>			
			<type>Selector</type>
			<xpath>normalize-space(.//div[@id='listing-details-list']/ul[@class='important-fields']/li/span[normalize-space(text())='Price / SqFt:']/following-sibling::strong[1]/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>price</name>
			<defaultValue>-1</defaultValue>			
			<type>Selector</type>
			<xpath>normalize-space(.//span[@id='actualprice']/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>		
		<Attribute>
			<name>building</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='normal-fields']/li/span[normalize-space(.)='Building']/following-sibling::strong[1]/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>property_reference</name>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='normal-fields']/li/span[normalize-space(.)='Property Reference']/following-sibling::strong[1]/text())</xpath>
		</Attribute>	
		<Attribute>
			<name>amenities</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(string(.//li[@class='amenities-list-element']/div/text()))</xpath>
		</Attribute>
		<Attribute>
			<name>ded_license_number</name>
			<defaultValue>NA</defaultValue>			
			<type>Selector</type>
			<xpath>normalize-space(.//div[@id='broker-details']//span[normalize-space(.)='DED Licence Number:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>rera_registration_number</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@id='broker-details']//span[normalize-space(.)='RERA Registration Number:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>trade_name</name>
			<defaultValue>NA</defaultValue>			
			<type>Selector</type>
			<xpath>normalize-space(.//span[@class='lister-name']/a/text())</xpath>
		</Attribute>		
		<Attribute>
			<name>ad_images</name>
			<defaultValue>NA</defaultValue>
			<type>Multiple</type>
			<xpath>.//div[@class='new-property fotorama']/img/@src</xpath>
		</Attribute>
		<Attribute>
			<name>description</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>			
			<xpath>normalize-space(string(.//div[@id='description-text']//span))</xpath>
		</Attribute>
		<Attribute>
			<name>area</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[contains(@class, 'location-areas')]/span[last()]/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<name>subarea</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
            <xpath>normalize-space(.//div[@id='listing-sidebar-col']/div[@id='map-wrapper']/div[@class='location']/div[@class='location-areas']/span[1]/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<name>type</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//span[@id='browse_in_breadcrumb']/div[@class='seo_itemscope'][4]/a/text())</xpath>
			<actions>
				<action>dubizzleExtractTypeForSale</action>
				<action>changeToSmallLetters</action>
			</actions>
		</Attribute>
        <Attribute>
            <name>longitude</name>
            <type>Selector</type>
            <defaultValue>NA</defaultValue>
            <xpath>.//script/text()[contains(., 'window.DETAILS_PAGE_MAP_GLOBALS =')]</xpath>
            <actions>
                <action>dubizzleGetLongitude</action>
            </actions>
        </Attribute>
        <Attribute>
            <name>latitude</name>
            <type>Selector</type>
            <defaultValue>NA</defaultValue>
            <xpath>.//script/text()[contains(., 'window.DETAILS_PAGE_MAP_GLOBALS =')]</xpath>
            <actions>
                <action>dubizzleGetLatitude</action>
            </actions>
        </Attribute>
        <Attribute>
            <name>buildingAndArea</name>
            <comment>This attribute is to be use in a pipeline for Dubbizle to handle the missing of the building attribute</comment>
            <type>Selector</type>
            <xpath>normalize-space(string(.//div[@class='location-areas']))</xpath>
            <defaultValue>NA</defaultValue>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
			<name>phone</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//a[@id='phone-lead']/@rel)</xpath>
		</Attribute>
    </DetailsAttributes>
</websiteInformation>