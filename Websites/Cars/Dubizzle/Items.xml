<websiteInformation>
	<domain>dubizzle.com</domain>
	<start_urls>
		<url>http://uae.dubizzle.com/motors/used-cars/</url>
	</start_urls>
	<FirstDate>25th Aug 2015</FirstDate>
	<LastDate>25th Aug 2012</LastDate>
	<Category>Car</Category>
	<Action>Sale</Action>
	<SpiderName>DubbizelCars</SpiderName>
	<Source>dubizzleproperty</Source>
	<LogFile>LogFiles/Car/Sale/cars_dubizle.log</LogFile>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>MasterPage</Place>
		<DateFunction>DayFirst</DateFunction>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>link</name>
				<xpath>normalize-space(.//span[@class='title']/a/@href)</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>posting_date</name>
				<xpath>normalize-space(.//div[@class='description']/ul[@class='feature']//p[@class='date']/text())</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//div[@id='results-list']/div[@class='list-item-wrapper']</xpath>
		</Containers>
		<NextPage>
			<xpath>.//div[@class='paging_forward']/a[@id='next_page']/@href</xpath>
			<prefix>http://uae.dubizzle.com/motors/used-cars/</prefix>
		</NextPage>
		<LinkAttribute>
			<name>link</name>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>link</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>posting_date</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>ad_title</name>
			<xpath>normalize-space(.//div[@class='block item-title']//span[@class='title']/a/text())</xpath>
			<type>Selector</type>
		</Attribute>
		<Attribute>
			<name>make</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Make:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>model</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Model:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>year</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Year:']/following-sibling::strong[1]/text())</xpath>
			<actions>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>kilometers</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Kilometers:']/following-sibling::strong[1]/text())</xpath>
			<regularExpression>(\d+(?:[.,]\d*)*)</regularExpression>
		</Attribute>
		<Attribute>
			<name>color</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Color:']/following-sibling::strong[1]/text())</xpath>
			<regularExpression>(\d+(?:[.,]\d*)*)</regularExpression>
		</Attribute>
		<Attribute>
			<name>doors</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='important-fields']/li/span[normalize-space(.)='Doors:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>body_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Body Condition:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>mechanical_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Mechanical Condition:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>seller_type</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Seller Type:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>motors_trim</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Motors Trim:']/following-sibling::strong[1]/text())</xpath>
			<encoding>ascii</encoding>
		</Attribute>
		<Attribute>
			<name>body_type</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Body Type:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>no_of_cylinders</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='No. Of Cylinders:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>transmission_type</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Transmission Type:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>horsepower</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Horsepowe:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>fuel_type</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Fuel Type:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>warranty</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Warranty:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>extras</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Extras:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>technical_features</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='listing-details-list']//ul/li/span[normalize-space(.)='Technical Features:']/following-sibling::strong[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>price</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//span[@id='actualprice']/text())</xpath>
			<actions>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>image_links</name>
			<type>Multiple</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//ul[@id='thumbs-list']/li/span/a/@href</xpath>
		</Attribute>
		<Attribute>
			<name>latitude</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='classified_detail_map']/following-sibling::script[1]/text())</xpath>
			<actions>
				<action>dubizzleGetLatitude</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>longitude</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@id='classified_detail_map']/following-sibling::script[1]/text())</xpath>
			<actions>
				<action>dubizzleGetLongitude</action>
			</actions>
		</Attribute>
	</DetailsAttributes>
</websiteInformation>