<websiteInformation>
	<domain>Autotraderuae.com</domain>
	<start_urls>
		<url>http://www.autotraderuae.com/cars</url>
	</start_urls>
	<FirstDate>25th Aug 2016</FirstDate>
	<LastDate>25th Aug 2012</LastDate>
	<Category>Car</Category>
	<Action>Sale</Action>
	<SpiderName>AutotraderCars</SpiderName>
	<Source>autotrader</Source>
	<LogFile>LogFiles/Car/Sale/cars_autotraderuae.log</LogFile>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>DetailsPage</Place>
		<DateFunction>DayFirst</DateFunction>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>ad_title</name>
				<xpath>normalize-space(.//div[@class='headText']/h2/a/text())</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>link</name>
				<xpath>normalize-space(.//div[@class='headText']/h2/a/@href)</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>kilometers</name>
				<xpath>normalize-space(.//div[@class='main-content-slider-text']//div[@class='listing-action-details']/ul//li/text()[contains(normalize-space(.),'km')])</xpath>
				<actions>
					<action>changeToFloat</action>
				</actions>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>year</name>
				<xpath>normalize-space(.//div[@class='main-content-slider-text']/div[@class='slider-content-text']/div[@class='slider-text-right']/ul/div//li/text()[contains(normalize-space(.),'Model')])</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//div[@class='main-content-slider ']</xpath>
		</Containers>
		<NextPage>
			<xpath>normalize-space(.//div[@class='pagination']//span[@class='pagin-box selected']/following-sibling::span[1]/a/text())</xpath>
			<prefix>http://www.autotraderuae.com/cars/?page=</prefix>
			<suffix>&amp;view=&amp;sort_by=edit_dt-desc&amp;limit=10&amp;query=&amp;car_make=&amp;car_version=&amp;color=&amp;seller_type=&amp;car_model=&amp;makeYear=&amp;city=</suffix>
			<action>autotraderNextPageAction</action>
		</NextPage>
		<LinkAttribute>
			<name>link</name>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>ad_title</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>link</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>kilometers</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>year</name>
			<type>Meta</type>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>posting_date</name>
			<xpath>normalize-space(.//div[@class='car-btm-dtls']/div[1]/span/span/text())</xpath>
			<type>Selector</type>
		</Attribute>
		<Attribute>
			<name>reference_number</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='car-btm-dtls']//div/span[contains(normalize-space(.),'Ad Refe')])</xpath>
		</Attribute>
		<Attribute>
			<name>price</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='car-slider-details']/div[@class='headText']//div/span[@itemprop='price']/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>make</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='features-input']/form/ul/li[1]/select/option[@selected]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>model</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='features-input']/form/ul/li[2]/select/option[@selected]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>version</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='features-input']/form/ul/li[3]/select/option[@selected]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>city</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='features-input']/form/ul/li[4]/select/option[@selected]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>color</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='features-input']/form/ul/li[7]/select/option[@selected]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>doors</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='fea-inside']/p/span[contains(normalize-space(.), 'No. of Doors:')]/following-sibling::span[1]/text())</xpath>
			<actions>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>body_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='fea-inside']/p/span[contains(normalize-space(.), 'Body Condition')]/following-sibling::span[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>car_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='fea-inside']/p/span[contains(normalize-space(.), 'Car Condition')]/following-sibling::span[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>drive_train</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='fea-inside']/p/span[contains(normalize-space(.), 'Drive Train')]/following-sibling::span[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>origin</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='fea-inside']/p/span[contains(normalize-space(.), 'Origin')]/following-sibling::span[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<name>model_feature</name>
			<type>Multiple</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='car-details-box']/ul/li/text()</xpath>
		</Attribute>
		<Attribute>
			<name>mobile</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='car-slider-details']/div[@class='headText']//div/span[@itemprop='telephone']/text())</xpath>
		</Attribute>
	</DetailsAttributes>
</websiteInformation>