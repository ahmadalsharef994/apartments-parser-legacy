<websiteInformation>
	<domain>uae.yallamotor.com</domain>
	<start_urls>
		<url>http://uae.yallamotor.com/used-cars/search</url>
	</start_urls>
	<FirstDate>7th Sep 2016</FirstDate>
	<LastDate>4th Sep 2012</LastDate>
	<Category>Car</Category>
	<Action>Sale</Action>
	<SpiderName>CarsYallamoto</SpiderName>
	<Source>yallamotor</Source>
	<LogFile>LogFiles/Car/Sale/cars_yallamoto.log</LogFile>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>MasterPage</Place>
		<DateFunction>MonthFirst</DateFunction>
		<action>yallamotorCorrectDate</action>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<name>link</name>
				<defaultValue>NA</defaultValue>
				<xpath>./div[@class='search-results-data']/h2/a/@href</xpath>
			</Attribute>
			<Attribute>
				<name>posting_date</name>
				<defaultValue>NA</defaultValue>
				<xpath>./div[@class='search-results-data']/span/font/text()</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//form[@id='search_compare_used_car']/div[@class='search-results-box clearfix']</xpath>
		</Containers>
		<NextPage>
			<xpath>.//a[@class='next_page']/@href</xpath>
			<prefix>http://uae.yallamotor.com</prefix>
		</NextPage>
		<LinkAttribute>
			<name>link</name>
			<prefix>http://uae.yallamotor.com/</prefix>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<name>link</name>
			<type>Meta</type>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>posting_date</name>
			<type>Meta</type>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>price</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='detail-page-head clearfix']/font/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>address</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='detail-page-head clearfix']/div[@class='detail-city']/text()</xpath>
		</Attribute>
		<Attribute>
			<name>city</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='City:']/following-sibling::span[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>area</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Area:']/following-sibling::span[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>make</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Make:']/following-sibling::span[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>model</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Model:']/following-sibling::span[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>version</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Version:']/following-sibling::span[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>year</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Year:']/following-sibling::span[1]/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>kilometers</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='used-car-detail-info']/ul/li/b[normalize-space(.)='Kilometers:']/following-sibling::span[1]/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>body_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Body Condition:']/following-sibling::text()</xpath>
		</Attribute>
		<Attribute>
			<name>mechanical_condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Mechanical Condition:']/following-sibling::text()</xpath>
		</Attribute>
		<Attribute>
			<name>Doors</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Doors:']/following-sibling::text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>cylinders</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Cylinders:']/following-sibling::text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>body_style</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Body Style:']/following-sibling::text()</xpath>
		</Attribute>
		<Attribute>
			<name>body_style</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@id='description']/strong[normalize-space(.)='Drive Type:']/following-sibling::text()</xpath>
		</Attribute>
		<Attribute>
			<name>image_links</name>
			<type>multible</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='es-carousel']/ul/li/a/img/@src</xpath>
		</Attribute>
	</DetailsAttributes>
</websiteInformation>