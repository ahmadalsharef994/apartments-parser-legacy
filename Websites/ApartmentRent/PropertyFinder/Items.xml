<websiteInformation>
    <domain>propertyfinder.ae</domain>
    <start_urls>
        <url>https://www.propertyfinder.ae/search?l=&amp;q=&amp;c=2&amp;t=&amp;rp=y&amp;pf=&amp;pt=&amp;bf=&amp;bt==&amp;at=&amp;fu=0&amp;kw=&amp;ob=nd</url>
    </start_urls>
    <FirstDate>25th Aug 2017</FirstDate>
   <LastDate>28th May 2016</LastDate>
    <Category>Apartment</Category>
    <Action>rent</Action>
    <SpiderName>PropertyFinderApartmentsRent</SpiderName>
    <Source>propertyfinder</Source>
    <TemplateAttributes>Websites/Templates/ApartmentsRent.xml</TemplateAttributes>
    <LogFile>LogFiles/Apartment/Rent/apartments_rent_propertyfinder.log</LogFile>
    <DateAttribute>
        <Place>DetailsPage</Place>
        <Name>posting_date</Name>
        <DateFunction>DayFirst</DateFunction>
        <action>getDatePropertyFinder</action>
    </DateAttribute>
    <MasterPage>
        <MasterAttributes>
            <Attribute>
                <defaultValue>NA</defaultValue>
                <name>ad_link</name>
                <xpath>.//div[@class='property-content']/h2/a/@href</xpath>
            </Attribute>
        </MasterAttributes>
        <Containers>
            <xpath>.//section[@id='serp']/ul/li[not(contains(@class,'agent-smart-ad-section'))]</xpath>
        </Containers>
        <NextPage>
            <xpath>.//nav[@class='pagination hide-on-print']/ul/li[@class='next pull-right']/a/@href</xpath>
            <prefix>https://www.propertyfinder.ae</prefix>
        </NextPage>
        <LinkAttribute>
            <name>ad_link</name>
			 <prefix>https://www.propertyfinder.ae</prefix>
        </LinkAttribute>
    </MasterPage>
    <DetailsAttributes>
        <Attribute>
            <name>posting_date</name>
            <type>Selector</type>
            <defaultValue>NA</defaultValue>
            <xpath>normalize-space(.//span[@class='last-date']/text())</xpath>
        </Attribute>
        <Attribute>
            <name>ad_title</name>
            <type>Selector</type>
            <defaultValue>Not Found</defaultValue>
            <xpath>normalize-space(string(.//div[@class='primary']/h1))</xpath>
        </Attribute>
        <Attribute>
            <name>ad_link</name>
            <defaultValue>NA</defaultValue>
            <type>Meta</type>
        </Attribute>
        <Attribute>
            <name>price</name>
            <type>Selector</type>
            <xpath>normalize-space(.//div[@class='price ']/span[@class='val']/text())</xpath>
            <defaultValue>-1</defaultValue>
            <actions>
                <action>extractNumbersFromString</action>
                <action>changeToFloat</action>
            </actions>
        </Attribute>
        <Attribute>
            <name>type</name>
            <type>Selector</type>
            <xpath>normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Type']/following-sibling::td[1]/text())</xpath>
            <defaultValue>NA</defaultValue>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>property_reference</name>
            <type>Selector</type>
            <xpath>
                normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Reference']/following-sibling::td[1]/text())
            </xpath>
        </Attribute>
        <Attribute>
            <defaultValue>-1</defaultValue>
            <name>bedrooms</name>
            <type>Selector</type>
            <xpath>
                normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Bedrooms']/following-sibling::td[1]/text())
            </xpath>
            <actions>
                <action>checkIfStudioBedroom</action>
                <action>changeToInteger</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>-1</defaultValue>
            <name>bathrooms</name>
            <type>Selector</type>
            <xpath>
                normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Bathrooms']/following-sibling::td[1]/text())
            </xpath>
            <actions>
                <action>changeToInteger</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>area</name>
            <type>Selector</type>
            <xpath>normalize-space(.//nav[@id='breadcrumbs']/span[2]/a/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>-1</defaultValue>
            <name>size</name>
            <type>Selector</type>
            <xpath>
                normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Area']/following-sibling::td[1]/text())
            </xpath>
            <actions>
                <action>changeToFloat</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>phone</name>
            <type>Selector</type>
            <xpath>normalize-space(//li[@class='phone']/a/@data-phone)</xpath>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>ad_images</name>
            <type>Multiple</type>
            <xpath>.//nav[@class='hide-on-print']/div/div/span/img/@src</xpath>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>trade_name</name>
            <type>Selector</type>
            <xpath>
                normalize-space(.//div[@class='broker']/div[@class='name']/div[normalize-space(.)='Company:']/following-sibling::div[1]/strong/text())
            </xpath>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>city</name>
            <type>Selector</type>
            <xpath>normalize-space(.//nav[@id='breadcrumbs']/span[1]/a/text())</xpath>
            <actions>
                <action>cleanCity</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>building</name>
            <type>Selector</type>
            <xpath>normalize-space(.//nav[@id='breadcrumbs']/span[last()]/a/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>UAE</defaultValue>
            <name>country</name>
            <type>Selector</type>
            <xpath>Nothing</xpath>
			<actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>payment_type</name>
            <type>Selector</type>
            <xpath>normalize-space(.//div[@class='price']/span[@class='period']/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <name>longitude</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>.//div[@class='map']/@data-lng</xpath>
        </Attribute>
        <Attribute>
            <name>latitude</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>.//div[@class='map']/@data-lat</xpath>
        </Attribute>
        <Attribute>
            <name>amenities</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>normalize-space(string(.//section[@id='property-amenities']/h3/following-sibling::ul))</xpath>
        </Attribute>
        <Attribute>
            <name>description</name>
            <type>Selector</type>
            <defaultValue>NA</defaultValue>
            <xpath>normalize-space(string(.//section[@id='page-content']))</xpath>
        </Attribute>
        <Attribute>
			<name>furnished</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//table[@class='fixed-table']//tr/th[normalize-space(.)='Furnishings']/following-sibling::td[1]/text())</xpath>
		</Attribute>
    </DetailsAttributes>
</websiteInformation>