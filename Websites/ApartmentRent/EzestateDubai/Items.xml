<websiteInformation>
	<domain>ezestate.com</domain>
	<start_urls>
		<url>https://ezestate.com/property-for-rent-dubai/properties/?sort_by=Date&amp;sort_type=desc</url>
	</start_urls>
	<FirstDate>25th Feb 2017</FirstDate>
	<LastDate>28th May 2016</LastDate>
	<Category>Apartment</Category>
	<Action>rent</Action>
	<SpiderName>apartments_rent_ezestatedubai</SpiderName>
	<Source>ezestate</Source>
	<TemplateAttributes>Websites/Templates/ApartmentsRent.xml</TemplateAttributes>
	<LogFile>LogFiles/Apartment/Rent/apartments_rent_ezestatedubai.log</LogFile>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>DetailsPage</Place>
		<DateFunction>DayFirst</DateFunction>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>ad_link</name>
				<xpath>.//div[@class='heading1 list_title']/a/@href</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>ad_title</name>
				<xpath>normalize-space(.//div[@class='heading1 list_title']/a/text())</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//div[@id='search_listing']/ul[@id='listings']/li[contains(@class,'item')]</xpath>
		</Containers>
		<NextPage>
			<xpath>normalize-space(.//li[@title='Next page']/a/@onclick)</xpath>
			<prefix>http://ezestate.com/property-for-rent-dubai/properties/?pg=</prefix>
			<action>ezestateParseNextPage</action>
		</NextPage>
		<LinkAttribute>
			<name>ad_link</name>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<name>posting_date</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//ul[@class='listingInfoWrap']/li/strong[normalize-space(.)='Listing date:']/following-sibling::span[1]/text())</xpath>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>ad_link</name>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>ad_title</name>
			<type>Meta</type>
		</Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>area</name>
            <type>Selector</type>
            <xpath>normalize-space(.//ul[@class='table property_details_list']/li/span[normalize-space(.)='Location:']/following-sibling::span[1]/a/text())</xpath>
			<actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
        <Attribute>
            <defaultValue>NA</defaultValue>
            <name>building</name>
            <type>Selector</type>
            <xpath>normalize-space(.//ul[@class='table property_details_list']/li/span[normalize-space(.)='Sub Community:']/following-sibling::span[1]/text())</xpath>
			<actions>
                <action>changeToSmallLetters</action>
            </actions>
        </Attribute>
		<Attribute>
			<defaultValue>UAE</defaultValue>
			<name>country</name>
			<type>Selector</type>
			<xpath>There is no xpath for this field</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<defaultValue>Dubai</defaultValue>
			<name>city</name>
			<type>Selector</type>
			<xpath>There is no xpath for this field</xpath>
			<actions>
				<action>cleanCity</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>bedrooms</name>
			<defaultValue>-1</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='info']/li[@class='bed']/text())</xpath>
			<actions>
                <action>checkIfStudioBedroom</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>bathrooms</name>
			<defaultValue>-1</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='info']/li[@class='bathroom']/text())</xpath>
			<actions>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>type</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='table property_details_list']/li/span[normalize-space(.)='Property Type:']/following-sibling::span[1]/text())</xpath>
            <actions>
                <action>changeToSmallLetters</action>
            </actions>
		</Attribute>
		<Attribute>
			<name>size</name>
			<defaultValue>-1</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='info']/li[@class='area last']/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>property_reference</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='listingInfoWrap']/li/strong[normalize-space(.)='Reference:']/following-sibling::span/text())</xpath>
		</Attribute>
		<Attribute>
			<name>price</name>
			<defaultValue>-1</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@class='details-left']/h2/a/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>price_sqft</name>
			<defaultValue>-1</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//ul[@class='table property_details_list']/li/span[normalize-space(.)='Price / Sq.ft:']/following-sibling::span[1]/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>amenities</name>
			<defaultValue>NA</defaultValue>
			<type>Multiple</type>
			<xpath>.//li[@class='amenities_details']/span[normalize-space(.)='Amenities:']/following-sibling::span[1]/ul//li/span/div/@title</xpath>
		</Attribute>
		<Attribute>
			<name>ad_images</name>
			<defaultValue>NA</defaultValue>
			<type>Multiple</type>
			<xpath>.//img[@u='thumb']/@src</xpath>
		</Attribute>
		<Attribute>
			<name>phone</name>
			<defaultValue>NA</defaultValue>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@id='agent_phone_no']/span/text())</xpath>
		</Attribute>
        <Attribute>
            <name>longitude</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>normalize-space(.//script[contains(text(),'addresses')]/text())</xpath>
            <actions>
				<action>ezestateGetLongitude</action>
			</actions>
        </Attribute>
        <Attribute>
            <name>latitude</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>normalize-space(.//script[contains(text(),'addresses')]/text())</xpath>
			<actions>
				<action>ezestateGetLatitude</action>
			</actions>
        </Attribute>
		<Attribute>
			<name>description</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(string(.//div[@id='description_expand']))</xpath>
		</Attribute>
		<Attribute>
			<name>payment_type</name>
			<type>Selector</type>
			<defaultValue>Yearly</defaultValue>
			<xpath>this website does not have so we will use the default value</xpath>
			<actions>
				<action>changeToSmallLetters</action>
			</actions>
		</Attribute>
        <Attribute>
            <name>trade_name</name>
            <defaultValue>NA</defaultValue>
            <type>Selector</type>
            <xpath>normalize-space(.//div[@class='seller_info list-summary']/ul/li/span[@class='name']/following-sibling::span/text())</xpath>
        </Attribute>
		<Attribute>
			<name>furnished</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>this website does not have this field</xpath>
		</Attribute>
    </DetailsAttributes>
</websiteInformation>