<websiteInformation>
	<ActionsFile>ApartmentsForRent\JustProperty\Actions.xml</ActionsFile>
	<domain>justproperty.com</domain>
	<start_urls>
		<url>The value is filled from inside the spider</url>
	</start_urls>
	<FirstDate>25th Feb 2015</FirstDate>
	<LastDate>24th Feb 2015</LastDate>
	<Category>Apartment</Category>
	<Action>Rent</Action>
	<SpiderName>JustPropertyApartmentsRent</SpiderName>
	<Source>JustProperty</Source>
	<LogFile>apartments_rent_justproperty_log.txt</LogFile>
	<DateAttribute>
		<Place>MasterPage</Place>
		<Name>last_updated</Name>
		<DateFunction>MonthFirst</DateFunction>
		<action>justpropertyDateAction</action>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>url</name>
				<xpath>.//div[@class='info-area']/h2/a/@href</xpath>
			</Attribute>
			<Attribute>
				<defaultValue>NA</defaultValue>
				<name>last_updated</name>
				<xpath>Nothing</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//section[@id='serp']/ul/li</xpath>
		</Containers>
		<NextPage>
			<xpath>.//nav[@id='pagination']/ul/li[@class='next']/a/@href</xpath>
			<prefix>http://www.justproperty.com</prefix>
		</NextPage>
		<LinkAttribute>
			<name>url</name>
			<prefix>http://www.justproperty.com</prefix>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<name>last_updated</name>
			<type>Meta</type>
			<defaultValue>Not Found</defaultValue>
		</Attribute>
		<Attribute>
			<name>link</name>
			<type>Meta</type>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>url</name>
			<defaultValue>NA</defaultValue>
			<type>Meta</type>
		</Attribute>
		<Attribute>
			<name>furnishings</name>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@class='details-amenities-extra']/div[normalize-space(.)='Furnishings:']/following-sibling::div[1]/text())</xpath>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>price_per__sqft</name>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@class='details-amenities-extra']/div[normalize-space(.)='Price per sq ft:']/following-sibling::div[1]/text())</xpath>
			<defaultValue>NA</defaultValue>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<defaultValue>NA</defaultValue>
			<name>plot_area</name>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@class='details-amenities-extra']/div[normalize-space(.)='Plot Area:']/following-sibling::div[1]/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>reference_no</name>
			<type>Selector</type>
			<xpath>normalize-space(.//div[@class='details-amenities-extra']/div[normalize-space(.)='Reference No:']/following-sibling::div[1]/text())</xpath>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>price</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='price']/text())</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>ad_title</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='detail-heading']/h1/text())</xpath>
		</Attribute>
		<Attribute>
			<name>location</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='detail-heading']/h2/text())</xpath>
		</Attribute>
		<Attribute>
			<name>sqft</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='details-amenities-area'])</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>bedroom</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='details-amenities-bedrooms'])</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>bathroom</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//div[@class='details-amenities-bathroom'])</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>mobile_number</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>normalize-space(.//a[@class='show_phone_number']/@data-phone)</xpath>
		</Attribute>
	</DetailsAttributes>
</websiteInformation>