<websiteInformation>
	<domain>carmudi.ae</domain>
	<start_urls>
		<url>http://www.carmudi.ae/cars/?sort=newest</url>
	</start_urls>
	<FirstDate>4th Sep 2014</FirstDate>
	<LastDate>4th Sep 2013</LastDate>
	<Category>Car</Category>
	<Action>Sale</Action>
	<SpiderName>CarmudiCars</SpiderName>
	<Source>Carmudi</Source>
	<LogFile>LogFiles/Car/Sale/cars_carmudi.log</LogFile>
	<DateAttribute>
		<Name>posting_date</Name>
		<Place>No Place</Place>
		<DateFunction>8943uoirjklsaf</DateFunction>
	</DateAttribute>
	<MasterPage>
		<MasterAttributes>
			<Attribute>
				<name>link</name>
				<defaultValue>NA</defaultValue>
				<xpath>.//a[@data-layer-action='title']/@href</xpath>
			</Attribute>
		</MasterAttributes>
		<Containers>
			<xpath>.//div[@class='catalog-item']</xpath>
		</Containers>
		<NextPage>
			<xpath>.//label[@class='field page-number']/input/@value</xpath>
			<prefix>http://www.carmudi.ae/cars/?sort=newestamppage=</prefix>
			<action>carmudiNextPageAction</action>
		</NextPage>
		<LinkAttribute>
			<name>link</name>
			<prefix>http://www.carmudi.ae/</prefix>
		</LinkAttribute>
	</MasterPage>
	<DetailsAttributes>
		<Attribute>
			<name>link</name>
			<type>Meta</type>
			<defaultValue>NA</defaultValue>
		</Attribute>
		<Attribute>
			<name>price</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='car-price']/h2/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToFloat</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>Color</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='medium-6 large-6 column']//tr/th[normalize-space(.)='Color Family']/following-sibling::td[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Year</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped']//tr/td[normalize-space(.)='Build (Year):']/following-sibling::td[1]/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>KM</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped']//tr/td[normalize-space(.)='Mileage (in km):']/following-sibling::td[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Transmission</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped']//tr/td[normalize-space(.)='Transmission:']/following-sibling::td[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Condition</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped']//tr/td[normalize-space(.)='Condition:']/following-sibling::td[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Fuel</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped']//tr/td[normalize-space(.)='Fuel:']/following-sibling::td[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>ListingID</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='property-info']/p[normalize-space(.)='Concerning the listing']/following-sibling::p[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>ad_title</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//div[@class='main-content']//header/h1/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Description</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//p[normalize-space(.)='Description']/following-sibling::p[1]/text()</xpath>
		</Attribute>
		<Attribute>
			<name>Seats</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped'][last()]//tr/td[normalize-space(.)='Seats']/following-sibling::td[1]/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
		<Attribute>
			<name>Doors</name>
			<type>Selector</type>
			<defaultValue>NA</defaultValue>
			<xpath>.//table[@class='table-striped'][last()]//tr/td[normalize-space(.)='Doors']/following-sibling::td[1]/text()</xpath>
			<actions>
				<action>extractNumbersFromString</action>
				<action>changeToInteger</action>
			</actions>
		</Attribute>
	</DetailsAttributes>
</websiteInformation>